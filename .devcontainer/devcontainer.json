{
    "name": "QuantumKatas Docker",
    "dockerFile": "Dockerfile",
    "extensions": [
        "quantum.quantum-devkit-vscode",
        "ms-vscode.csharp",
        "donjayamanne.python-extension-pack"
    ],
    "postStartCommand": "./devcontainer/config.sh",
    "runArgs": [
        "-p",
        "8888:8888"
    ],
    "postAttachCommand": "jupyter notebook --port=8888 --no-browser --ip=0.0.0.0"
}
